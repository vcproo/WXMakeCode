/**app.wxss**/
.container {height: 100%; display: flex; flex-direction: column; align-items: center; justify-content: space-between; padding: 200rpx 0; box-sizing: border-box;}
/**标题栏样式**/
.top{float: left; width: 92%; padding: 40px 4% 0; position: fixed; top:0; background: #35bffe; z-index: 9;}
.top_01{float: left; width: 30%;} 
.top_01 a{float: left; width: 100%; padding: 14px 0 0 0;} 
.top_01 a image{ width: 12px; height: 22px;} 
.top_02{float: left; width: 40%; text-align: center; font-size: 19px; line-height: 50px; color: #ffffff;} 
.top_03{float: left; width: 30%; text-align: right;} 
.top_03 a{float: left; width: 100%; padding: 14px 0; color: #000000; font-size: 16px;} 
.top_03 a image{ width: 21px; height: 14px; margin-right: 3px;} 
.empty_custom{height: 45px; width: 100%;}
