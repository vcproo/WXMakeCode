/* pages/infolist/infolist.wxss */
body, h1, h2, h3, h4, h5, h6, hr, p, blockquote, dl, dt, dd, ul, ol, li, pre, form, fieldset, legend, button, input, textarea, img{border:medium none;margin:0;padding:0;font-family: "微软雅黑";}
ul, ol, li{list-style:none}
a,a:hover{text-decoration:none;}
/* @media screen and (max-device-width: 320px){} */
.qblb{float: left; width: 94%; padding: 0 3%; margin-bottom: 20px;}
.qblb_n{float: left; width: 100%; border-radius: 8px; border: 1px solid #dfdfdf; margin-top: 20px;}
.qblb_n1{float: left; width: 96%; padding: 0px 2%; background: #f1f1f1; border-radius: 8px 8px 0 0;}
.qblb_n1 p{ float: left; width: 24.7%; text-align: center; line-height: 23px; font-size: 15px; border-right: 1px solid #e4e4e4; padding: 12px 0;}
.qblb_n1 p:last-child{border-right: none;}
.qblb_n2{float: left; width: 96%; padding: 0px 2%; border-radius: 8px 8px 0 0; border-bottom: 1px solid #e4e4e4;}
.qblb_n2 p{ float: left; width: 24.7%; text-align: center; line-height: 23px; font-size: 14px; border-right: 1px solid #e4e4e4; padding: 12px 0;}
.qblb_n2 p:last-child{border-right: none;}
.qblb_n3{float: left; width: 100%; text-align: center; padding: 15px 0%; background: #ffffff; border-radius: 8px;}
.qblb_n3 a{float: left; width: 45%; background: #35bffe; border-radius: 8px; line-height: 50px; height: 50px; color: #ffffff; margin-left: 3%;}
.qblb_n4{float: left; width: 90%; padding: 10px 5%; border-bottom: 1px dashed #c7c7c7; line-height: 24px;}
.qblb_n4 text{float: left; color: #000000;}
.qblb_n4 p{float: left; color: #555555;}